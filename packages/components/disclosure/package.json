    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@react-dive-ui/tsconfig": "workspace:*",
    "@types/lodash.isequal": "^4.5.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.46.0",
    "eslint-config-dive": "workspace:*",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@react-dive-ui/disclosure-machine": "workspace:*",
    "@react-dive-ui/compose-event-handlers": "workspace:*",
    "@react-dive-ui/dive": "workspace:*",
    "@react-dive-ui/merge-props": "workspace:*",
    "@react-dive-ui/use-previous-value": "workspace:*",
    "@react-dive-ui/use-isomorphic-layout-effect": "workspace:*",
    "@xstate/react": "4.0.0-beta.8",
    "lodash.isequal": "^4.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
